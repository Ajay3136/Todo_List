<%- include("header") -%>
  <div class="box" id="heading">
    <h1><%= listTitle %> </h1>
  </div>

  <div class="box">
      <% for(var i=0; i<addItem.length; i++){ %>
      <div class="item">
        <input type="checkbox">
        <p> <%= addItem[i] %></p>
      </div>
      <% } %>
    <form autocomplete="off" action="/" method="post">

        <input autocomplete="off" type="text" name="newItem" placeholder="New Item">

        <button type="submit" name="list" value=<%= listTitle %>>+</button>

    </form>
  </div>
<%- include("footer") -%>
